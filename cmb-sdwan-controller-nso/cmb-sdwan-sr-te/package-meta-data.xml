<ncs-package xmlns="http://tail-f.com/ns/ncs-packages">
	<name>cisco-nso-cmb-sdwan-sr-te</name>
	<package-version>1.0</package-version>
	<description>Java- and template-based cisco-nso-cmb-sdwan-sr-te
		resource facing service
	</description>
	<ncs-min-version>4.4</ncs-min-version>

	<!-- It's possible to add more components to the -->
	<!-- same package, multiple services, data providers etc -->

	<component>
		<name>CmbSdwanSrTeService</name>
		<callback>
			<java-class-name>com.cisco.cmb.sdwan.CmbSdwanSrTeService
			</java-class-name>
		</callback>
	</component>

	<component>
		<name>ValidateInterfaceStatusAction</name>
		<callback>
			<java-class-name>
				com.cisco.cmb.sdwan.action.ValidateInterfaceStatusAction
			</java-class-name>
		</callback>
	</component>
</ncs-package>
